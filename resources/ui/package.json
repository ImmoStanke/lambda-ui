{
  "name": "lambda-ui",
  "version": "0.1.0",
  "description": "Modern UI for LambdaCD",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass-once": "node-sass --include-path sass --output public/css sass",
    "sass-watch": "nodemon -e sass -x \"npm run sass-once\"",
    "clean": "rm -rf public/css public/js",
    "cleanNode": "rm -rf node_modules",
    "babel-watch": "node_modules/babel-cli/bin/babel.js --watch js-src --out-dir public/js",
    "babel-once": "node_modules/babel-cli/bin/babel.js  js-src --out-dir public/js",
    "js-once": "npm run babel-once && node_modules/webpack/bin/webpack.js public/js/index.js public/js/bundle.js",
    "js-watch": "nodemon -e jsx,es6 -x \"npm run js-once\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sroidl/lambda-ui.git"
  },
  "keywords": [
    "lambdacd",
    "continuous",
    "delivery",
    "user",
    "interface",
    "continuous",
    "integration",
    "ci",
    "cd"
  ],
  "author": "Sebastian Roidl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sroidl/lambda-ui/issues"
  },
  "homepage": "https://github.com/sroidl/lambda-ui#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
